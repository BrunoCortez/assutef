-- Executar estes comandos no PGAdmin, após o lift no sails.

-- Cria tabela de Vinculos das contas

CREATE TABLE VinculoPlanoConta (
  ID 				BIGSERIAL NOT NULL,   
  DS_Operacao 			VARCHAR(60), 
  PCReceitaID 			int8, 
  PCDespesaID 			int8, 
  PRIMARY KEY (ID));

----------------------------------------------------------  

-- Implementação das FK's
  
ALTER TABLE Conveniado ADD CONSTRAINT pkConveniadoTipo_Conveniado FOREIGN KEY (tipoconveniado) REFERENCES TipoConveniado (ID);  
ALTER TABLE Conveniado ADD CONSTRAINT pkEndereco_Conveniado FOREIGN KEY (endereco) REFERENCES Endereco (ID);
ALTER TABLE Conveniado ADD CONSTRAINT pkCC_Conveniado FOREIGN KEY (contacorrente) REFERENCES ContaCorrente (ID);
ALTER TABLE Associado ADD CONSTRAINT pkUsuario_Associado FOREIGN KEY (usuario) REFERENCES Usuario (ID);
ALTER TABLE Associado ADD CONSTRAINT pkEndereco_Associado FOREIGN KEY (endereco) REFERENCES Endereco (ID);
ALTER TABLE Produto ADD CONSTRAINT pkCategoria_Produto FOREIGN KEY (categoria) REFERENCES Categoria (ID);
ALTER TABLE ContaCorrente ADD CONSTRAINT pkBanco_CC FOREIGN KEY (banco) REFERENCES Banco (ID);
ALTER TABLE ContaCorrente ADD CONSTRAINT pkPlanoConta_CC FOREIGN KEY (planoconta) REFERENCES PlanoConta (ID);
ALTER TABLE Chequinho ADD CONSTRAINT pkAssociado_Cheque FOREIGN KEY (associado) REFERENCES Associado (ID);
ALTER TABLE VinculoPlanoConta ADD CONSTRAINT pkPlanoConta_DespesaVinc FOREIGN KEY (PCDespesaID) REFERENCES PlanoConta (ID);
ALTER TABLE VinculoPlanoConta ADD CONSTRAINT pkPlanoConta_ReceitaVinc FOREIGN KEY (PCReceitaID) REFERENCES PlanoConta (ID);
ALTER TABLE Venda ADD CONSTRAINT pkAssociado_Venda FOREIGN KEY (associado) REFERENCES Associado (ID);
ALTER TABLE VendaProduto ADD CONSTRAINT pkVenda_VP FOREIGN KEY (venda) REFERENCES Venda (ID);
ALTER TABLE VendaProduto ADD CONSTRAINT pkProduto_VP FOREIGN KEY (produto) REFERENCES Produto (ID);
ALTER TABLE Lancamento ADD CONSTRAINT pkLancamento_Conveniado FOREIGN KEY (conveniado) REFERENCES Conveniado (ID);
ALTER TABLE Lancamento ADD CONSTRAINT pkLancamento_Associado FOREIGN KEY (associado) REFERENCES Associado (ID);

----------------------------------------------------------

-- Implementação da estrutura do nivel de ativos do plano de Contas 

